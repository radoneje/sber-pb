          .aboutForm
            .aboutFormTitle !{site.about[lang]}
            .aboutFormLid !{content.lid[lang]}
            .aboutFormItems
              each item in content.list
                .aboutFormItem !{item[lang]}

          .aboutForm
            .aboutFormTitle !{site.speakers[lang]}
            .speakers
              each speaker in speakers
                .speaker(vv-on:click="showSpeaker("+JSON.stringify(speaker)+")")
                  .speakerPhoto !{"<img src='/images/"+speaker.img+"'>"}
                  .sbeakerName !{speaker.name[lang]}
                  .speakerPos !{speaker.position[lang]}
          .aboutForm
            .aboutFormLid !{content.addText[lang]}

          //.aboutForm
            .aboutFormTitle !{lang=='ru'?'Архив':"Archive"}
            .archiveItemBox
                .archiveItem
                  if lang=='ru'
                    .archiveTitle.sbeakerName Инвестиции будущего: Брилиантовая революция
                    .archiveVideoWr.videoWrapper
