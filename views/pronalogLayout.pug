    div#app(v-bind:class="{isLogin:isLogin, isPlayer:isPlayer}")
      //div.loader(v-if="!loaded")
        //img(src="/images/loader2.gif"aboutFormButton)

      div.work(v-if="loaded" v-bind:class="{visible:loaded}" )

        header
          .bumper
            .tower
              .mainLogo
                include  logo.pug




              //.mainLang
                span.mainLangItem(class=""+lang=="ru" onclick="changeLang('ru')")  ru
                span.mainLangDivaider &nbsp;|&nbsp;
                span.mainLangItem(class=""+lang=="en" onclick="changeLang('en')") en
              .playerTitle(v-if="isPlayer")
                include titleProNalog.pug

            //.gradient
            //.mainMenu
              .mainMenuItem.active Регистрация
              .mainMenuItem О мероприятии
              .mainMenuItem Спикеры
              .mainMenuItem Программа

        .tower
          .mainContent(v-bind:class="{player:isPlayer}")
            .mainTitle()
              include titleProNalog

        .regForm(v-if="!isPlayer ")
          .regFormHeader()
            div(v-if="stage<=1") !{site.registration[lang]}
            div(v-if="stage==10") !{site.registrationComplite[lang]}
            div(v-if="stage==3 || stage==4") !{site.autorisation[lang]}
          .regFormBody#regFormBody
            block content
        .tower
          .aboutForm
            .palyerWr
              .playerHead
                .playerLogo
                  include  logo.pug

              //.playerBody.a

                .playerObj
                  .videoWrapper
                    if lang=="ru"
                      <iframe width="560" height="315" src="https://www.youtube.com/embed/9UdD8ftyLSE" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                    else
                      <iframe width="560" height="315" src="https://www.youtube.com/embed/434ciYDLqsU" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                    //iframe( src="https://front.sber.link/playerpb"+lang+".html" border="0" width="640" height="360" frameborder="0" allow="autoplay; fullscreen; picture-in-picture" allowfullscreen)
                //include playerCtrl.pug
                //.playerChatBox
                  include playerCtrl.pug
                  .playerChatWr(v-if="!chatIsActive")

                    .playerChatPole#qBox
                      .playerChatPoleWr
