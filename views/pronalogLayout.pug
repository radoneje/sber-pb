                            //.playerChatTitle {{item.i}} {{item.o}}
                            .playerChatText {{item.text}}
                            .playerChatDate {{moment(item.date).format('HH:mm')}}
                          .playerChatItem(v-if="q.filter(c=>!c.isDeleted).length==0")
                            .playerChatText {{lang=="ru"?"нет сообщений":"no messages"}}
                    .playerChatInputForm
                      input(placeholder=site.qPlaceholder[lang] v-model="qText" maxlength="1200" v-on:keyup.enter="newQ()")
                      .btn.playerChatSend(v-on:click="newQ()")
